<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@dashboardDesign.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.InscriptionConfirmer">
   <children>
      <VBox layoutX="21.0" layoutY="10.0" prefHeight="360.0" prefWidth="557.0" spacing="10.0">
         <children>
            <Label prefHeight="51.0" prefWidth="347.0" styleClass="titre-grand" text="Inscription validée" />
            <HBox spacing="10.0">
               <children>
                  <Label styleClass="titre" text="Formation : " />

                  <Label fx:id="nomFor" text="Label">
                     <HBox.margin>
                        <Insets top="3.0" />
                     </HBox.margin></Label>
               </children>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Label styleClass="titre" text="Date de Début : " />

                  <Label fx:id="dateD" text="Label">
                     <HBox.margin>
                        <Insets top="3.0" />
                     </HBox.margin></Label>
               </children>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Label styleClass="titre" text="Date de Fin : " />

                  <Label fx:id="dateF" text="Label">
                     <HBox.margin>
                        <Insets top="3.0" />
                     </HBox.margin></Label>
               </children>
            </HBox>
            <Button fx:id="imprimerButton" mnemonicParsing="false" text="Imprimer" />
            <ImageView fx:id="qrCodeImageView" fitHeight="200.0" fitWidth="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
